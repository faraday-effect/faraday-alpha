{
  "name": "designer",
  "version": "0.0.1",
  "description": "Design application for Faraday",
  "productName": "Faraday Designer",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Tom Nurkkala <tnurkkala@cse.taylor.edu>",
  "private": true,
  "scripts": {
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "lint": "eslint --ext .js,.vue src",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "test:e2e:CI": "cypress run",
    "test:e2e": "cypress open",
    "test:unit:coverage": "jest --coverage",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:unit:ui": "majestic",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "test:unit": "jest"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.0",
    "codemirror": "^5.43.0",
    "mousetrap": "^1.6.2",
    "quasar": "^1.0.0-beta.0",
    "reflect-metadata": "^0.1.13",
    "rehype-format": "^2.3.1",
    "rehype-highlight": "^2.2.1",
    "rehype-stringify": "^5.0.0",
    "remark-parse": "^6.0.3",
    "remark-rehype": "^4.0.0",
    "unified": "^7.1.0",
    "vue-property-decorator": "^8.1.1",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-beta.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0-rc.8",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0-beta.23",
    "@quasar/quasar-app-extension-typescript": "^1.0.0-alpha.14",
    "@types/codemirror": "^0.0.74",
    "@types/node": "11.9.5",
    "@types/vuelidate": "^0.7.5",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.1.0",
    "majestic": "^1.2.24",
    "prettier": "^1.16.4",
    "strip-ansi": "=3.0.1",
    "ts-jest": "^24.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  }
}
