    "pg": "^7.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "type-graphql": "^0.17.1",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.1.1",
    "@types/express": "^4.16.1",
